<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="partyScene" prefHeight="400.0" prefWidth="600.0" 
			xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" 
			fx:controller="application.Controller" >
   <children>
      <ImageView fx:id="icon" fitHeight="171.0" fitWidth="170.0" layoutX="14.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true" />
      <ListView fx:id="poliInParty" layoutX="257.0" layoutY="153.0" prefHeight="200.0" prefWidth="297.0" />
      <Label fx:id="partyName" layoutX="286.0" layoutY="41.0" prefHeight="42.0" prefWidth="201.0" text="Label" />
       <MenuBar>
           <menus>
               <Menu fx:id="option" mnemonicParsing="false" text="Options">
                   <items>
                       <MenuItem fx:id="homeParty" text="Home" onAction="#goToHome"/>
                       <MenuItem fx:id="searchParty" text="Search" onAction="#goToSearch"/>
                       <MenuItem text="Exit" onAction="#exitApp"/>
                   </items>
               </Menu>
           </menus>
       </MenuBar>
   </children>
</AnchorPane>
